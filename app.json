{
  "expo": {
    "name": "KutumbaMara",
    "slug": "KutumbaMara",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/kutumba-icon.png",
    "scheme": "kutumbamara",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/kutumba-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000B1E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.KutumbaMara"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kutumba-icon.png",
        "backgroundColor": "#000B1E"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.KutumbaMara"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}